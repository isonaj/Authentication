@{
    ViewData["Title"] = "Home Page";
}
@using Microsoft.AspNetCore.Authentication;

<div class="row">
    <div class="col-md-12">
        <h2>Claims</h2>

        <dl>
            @foreach (var claim in User.Claims)
            {
                <dt>@claim.Type</dt>
                <dd>@claim.Value</dd>
            }
        </dl>

    </div>
</div>
